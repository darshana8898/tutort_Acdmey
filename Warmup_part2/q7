7. https://leetcode.com/problems/majority-element

class Solution {
public:
    int majorityElement(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        return nums[(nums.size())/2];
    }
};

OR 
 int majorityElement(vector<int>& nums) {
int ele=arr[0];
int count=1;

for(int i=1;i<nums.size();i++)
{
  if(!count)
  ele=nums[i]

ele==nums[i]?count++:count--;
}
return ele;
}
